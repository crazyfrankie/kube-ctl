{
    "swagger": "2.0",
    "info": {
        "contact": {}
    },
    "paths": {
        "/api/pod": {
            "post": {
                "description": "创建新的Pod或更新已存在的Pod",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Pod管理"
                ],
                "summary": "创建或更新Pod",
                "parameters": [
                    {
                        "description": "Pod配置信息",
                        "name": "pod",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/req.Pod"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "参数错误(code=20001)或验证错误(code=20002)或系统错误(code=30000)",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            },
            "delete": {
                "description": "删除指定命名空间下的指定Pod",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Pod管理"
                ],
                "summary": "删除Pod",
                "parameters": [
                    {
                        "type": "string",
                        "description": "命名空间",
                        "name": "namespace",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Pod名称",
                        "name": "name",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "系统错误(code=30000)",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/pod/detail": {
            "get": {
                "description": "获取指定命名空间下指定Pod的详细信息",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Pod管理"
                ],
                "summary": "获取Pod详情",
                "parameters": [
                    {
                        "type": "string",
                        "description": "命名空间",
                        "name": "namespace",
                        "in": "query",
                        "required": true
                    },
                    {
                        "type": "string",
                        "description": "Pod名称",
                        "name": "name",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "系统错误(code=30000)",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/pod/list": {
            "get": {
                "description": "获取指定命名空间下的所有Pod列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Pod管理"
                ],
                "summary": "获取Pod列表",
                "parameters": [
                    {
                        "type": "string",
                        "description": "命名空间",
                        "name": "namespace",
                        "in": "query",
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "系统错误(code=30000)",
                        "schema": {
                            "$ref": "#/definitions/response.Response"
                        }
                    }
                }
            }
        },
        "/api/pod/namespace": {
            "get": {
                "description": "获取所有可用的命名空间列表",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "Pod管理"
                ],
                "summary": "获取命名空间列表",
                "responses": {
                    "200": {
                        "description": "返回命名空间列表，每个命名空间包含名称、创建时间和状态",
                        "schema": {
                            "allOf": [
                                {
                                    "$ref": "#/definitions/response.Response"
                                },
                                {
                                    "type": "object",
                                    "properties": {
                                        "data": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/definitions/resp.Namespace"
                                            }
                                        }
                                    }
                                }
                            ]
                        }
                    }
                }
            }
        }
    },
